{
  "name": "react-loadbar",
  "version": "0.1.2",
  "private": false,
  "author": "John Bernardo",
  "repository": {
    "type": "git",
    "url": "https://github.com/noisycr1cket/react-loadbar"
  },
  "license": "MIT",
  "keywords": [
    "progress",
    "loading",
    "react",
    "loader",
    "bar",
    "spinner",
    "bootstrap",
    "youtube",
    "material",
    "dom"
  ],
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "react": ">=15"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>jestSetup.js",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverageFrom": [
      "dist/*.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "prepush": "./pre",
    "lintAll": "yarn tslint . && yarn eslint .",
    "clean": "rm -rf build dist",
    "dist": "yarn clean && yarn build",
    "build:dev": "webpack --config webpack.config.dev.js",
    "build": "webpack --config webpack.config.prod.js",
    "buildCI": "IS_CI=true webpack --config webpack.config.prod.js",
    "test": "jest test --no-watchman --onlyChanged --coverage --coverageDirectory .coverage/",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -c .storybook -o .out"
  },
  "devDependencies": {
    "@baristalabs/react-scripts-ts": "2.9.5",
    "@storybook/addon-actions": "^3.3.3",
    "@storybook/addon-links": "^3.3.3",
    "@storybook/addons": "^3.3.3",
    "@storybook/react": "^3.3.3",
    "@types/enzyme": "^3.1.6",
    "@types/jest": "^22.0.0",
    "@types/react": "^16.0.31",
    "@types/storybook__react": "^3.0.5",
    "autoprefixer": "^9.4.8",
    "awesome-typescript-loader": "^3.4.1",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.0.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "codecov": "^3.0.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.14.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "prop-types": "^15.6.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.0",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  }
}
